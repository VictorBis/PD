{
  "name": "vke-website",
  "version": "1.0.0",
  "description": "Sitio web de VKE con servidor local para desarrollo",
  "main": "server.js",
  "scripts": {
    "build": "sass css/style.scss css/style.css --style=compressed",
    "build:watch": "sass css/style.scss css/style.css --watch",
    "start": "npm run build && node server.js",
    "dev": "npm run build:watch & node server.js",
    "vercel-build": "npm run build",
    "python": "python3 server.py"
  },
  "keywords": ["website", "development", "server"],
  "author": "VKE",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sass": "^1.70.0"
  }
}